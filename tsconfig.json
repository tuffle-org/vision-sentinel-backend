{
    "compilerOptions": {
        "target": "es6",
        "module": "commonjs" ,
        "outDir": "./dist" ,
        "strict": true ,
        "esModuleInterop": true ,
        "skipLibCheck": true ,
        "forceConsistentCasingInFileNames": true
    },
    "include": ["src/**/*.ts"] /* Specify which files to include. */,
    "exclude": ["node_modules"] /* Specify which files to exclude. */
}
